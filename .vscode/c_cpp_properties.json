{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/includes",
                "${workspaceFolder}/libopencm3/include",
                "${workspaceFolder}/FreeRTOS-Kernel/include",
                "${workspaceFolder}/FreeRTOS-Kernel/portable/GCC/ARM_CM3",
                "${workspaceFolder}/libopencm3/include/libopencm3/stm32",
				"${workspaceFolder}/libopencm3/include/libopencm3/stm32/f1"
            ],
            "defines": [
                "STM32F1",
				"configUSE_PREEMPTION=1",
				"configUSE_IDLE_HOOK=0",
				"configUSE_TICK_HOOK=0",
				"configMAX_PRIORITIES=3",
				"configUSE_TRACE_FACILITY=0",
				"configUSE_16_BIT_TICKS=0",
				"configIDLE_SHOULD_YIELD=1",
				"configUSE_MUTEXES=1",
				"configUSE_CO_ROUTINES=0",
				"configMAX_CO_ROUTINE_PRIORITIES=2",
				"configSUPPORT_STATIC_ALLOCATION=0",
				"configSUPPORT_DYNAMIC_ALLOCATION=1",
				"INCLUDE_vTaskPrioritySet=1",
				"INCLUDE_uxTaskPriorityGet=1",
				"INCLUDE_vTaskDelete=1",
				"INCLUDE_vTaskCleanUpResources=0",
				"INCLUDE_vTaskSuspend=1",
				"INCLUDE_vTaskDelayUntil=1",
				"INCLUDE_vTaskDelay=1",
				"configKERNEL_INTERRUPT_PRIORITY=255",
				"configMAX_SYSCALL_INTERRUPT_PRIORITY=191"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}